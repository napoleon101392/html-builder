{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 2%"
        }
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
